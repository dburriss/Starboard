{
  "apiVersion": "v1",
  "items": [
    {
      "apiVersion": "apps/v1",
      "kind": "Deployment",
      "metadata": {
        "name": "test-deployment",
        "namespace": "default"
      },
      "spec": {
        "minReadySeconds": 0,
        "progressDeadlineSeconds": 600,
        "replicas": 3,
        "revisionHistoryLimit": 10,
        "template": {
          "spec": {
            "containers": [
              {
                "args": [
                  "config",
                  "nginx"
                ],
                "command": [
                  "systemctl"
                ],
                "image": "nginx"
              }
            ]
          }
        }
      }
    }
  ],
  "kind": "List"
}